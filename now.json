{
    "version": 2,
    "name": "rypdt",
    "env": {
        "db": "@pdt_prod_db"
    },
    "alias": [
        "rypdt"
    ],
    "type": "static",
    "builds": [{
            "src": "./package.json",
            "use": "@now/static-build"
        },
        {
            "src": "./server/index.js",
            "use": "@now/node-server"
        }
    ],
    "routes": [{
            "src": "/api/*",
            "dest": "./server/index.js"
        },
        {
            "src": "/",
            "dest": "index.html"
        }
    ],
    "public": true
}